<!--index.wxml-->
<view class="container">
  <swiper indicator-dots="{{indicatorDots}}"
          autoplay="{{autoplay}}" 
          interval="{{interval}}" 
          duration="{{duration}}">
    <block wx:for="{{imgUrls}}" wx:key="imgUrlsId">
        <swiper-item>
          <image src="{{item}}"/>
        </swiper-item>
    </block>
  </swiper>

  <button class="mybutton" hover-class="mybutton-hover" bindtap="goToShop"> 现在开始买猪*(oo)*  </button>
</view>
